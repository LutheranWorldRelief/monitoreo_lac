{
  "name": "yiisoft/yii2-app-basic",
  "description": "Monitoreo LWR",
  "keywords": [
    "yii2",
    "framework",
    "basic",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "yiisoft/yii2": "2.0.15.1",
    "yiisoft/yii2-bootstrap": "2.0.8",
    "yiisoft/yii2-swiftmailer": "2.1.0",
    "yiisoft/yii2-authclient": "2.1.5",
    "yiisoft/yii2-jui": "2.0.7",
    "kartik-v/yii2-widget-select2": "2.1.1",
    "kartik-v/yii2-grid": "3.1.7",
    "kartik-v/yii2-mpdf": "1.0.2",
    "kartik-v/yii2-widgets": "3.4.0",
    "kartik-v/yii2-detail-view": "1.7.6",
    "kartik-v/yii2-helpers": "1.3.6",
    "kartik-v/yii2-date-range": "1.6.8",
    "kartik-v/yii2-builder": "1.6.3",
    "kartik-v/yii2-export": "1.2.9",
    "kartik-v/yii2-editable": "1.7.6",
    "kartik-v/yii2-widget-colorinput": "1.0.3",
    "kartik-v/yii2-widget-fileinput": "1.0.6",
    "bedezign/yii2-audit": "1.1.1",
    "dmstr/yii2-adminlte-asset": "2.6.0",
    "drsdre/yii2-wizardwidget": "1.2.3",
    "mishamosher/yii2-admin": "~2.0",
    "rmrevin/yii2-fontawesome": "2.17.1",
    "phpoffice/phpspreadsheet": "1.2.0",
    "yii2mod/yii2-array-query": "1.3",
    "coksnuss/yii2-gii-modelgen": "^2.0",
    "kartik-v/yii2-tabs-x": "1.2.4",
    "softark/yii2-dual-listbox": "1.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.0.13",
    "yiisoft/yii2-gii": "2.0.7",
    "yiisoft/yii2-faker": "2.0.4",
    "codeception/base": "2.4.0",
    "codeception/verify": "1.0.0",
    "codeception/specify": "1.0.0"
  },
  "config": {
    "process-timeout": 1800,
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
