SELECT
	*
FROM
	data_list
WHERE
	id IN (
SELECT
	pa.id
FROM
	data_list pa
	LEFT JOIN data_list li ON pa.list_id = li.id
WHERE
	li.slug = 'countries'
	AND pa.`value` NOT IN ( SELECT country FROM contact WHERE country IS NOT NULL GROUP BY country )
	AND pa.id NOT IN ( SELECT country_id FROM `event` WHERE country_id IS NOT NULL GROUP BY country_id union select country_id from organization
where country_id is not null	GROUP BY country_id )

	)